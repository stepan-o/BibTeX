issn = {1050-0472},
journal = {Journal of Mechanical Design},
number = {9},
pages = {091010},
title = {{Multi-Objective Wind Farm Layout Optimization Considering Energy Generation and Noise Propagation With NSGA-II}},
url = {http://mechanicaldesign.asmedigitalcollection.asme.org/article.aspx?doi=10.1115/1.4027847},
volume = {136},
year = {2014}
}
