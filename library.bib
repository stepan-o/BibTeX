institution = {University of Toronto Department of Economics},
pages = {1--30},
title = {{The Economics of Renewable Electricity Policy in Ontario}},
url = {http://www.economics.utoronto.ca/public/workingPapers/tecipa-478.pdf{\%}5Cnpapers2://publication/uuid/01EC7B64-00DA-4ACB-9F6B-EAC9525058F6},
year = {2013}
