pmid = {11688438},
title = {{Nutrition and diet for healthy lifestyles in Europe: science and policy implications.}},
url = {http://ec.europa.eu/health/archive/ph{\_}determinants/life{\_}style/nutrition/report01{\_}en.pdf},
volume = {4},
